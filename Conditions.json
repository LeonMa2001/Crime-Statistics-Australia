{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Proportion of people with long-term health conditions (%) across states",
    "data": {
        "url": "https://raw.githubusercontent.com/LeonMa2001/Healthsite/refs/heads/main/Proportion%20of%20people%20with%20long-term%20health%20conditions%20(%25).csv",
        "format": {
            "type": "csv"
        }
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "State",
            "type": "nominal",
            "axis": {"title": "State"}
        },
        "y": {
            "field": "Percentage",
            "type": "quantitative",
            "axis": {"title": "Percentage (%)"}
        },
        "color": {
            "field": "Condition",
            "type": "nominal",
            "legend": {"title": "Condition"}
        }
    }
}
